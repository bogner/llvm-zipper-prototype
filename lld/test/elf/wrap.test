      Section:         .data
    - Name:            .bss
      Type:            STT_SECTION
      Section:         .bss
    - Name:            .comment
      Type:            STT_SECTION
      Section:         .comment
    - Name:            .note.GNU-stack
      Type:            STT_SECTION
      Section:         .note.GNU-stack
    - Name:            .eh_frame
      Type:            STT_SECTION
      Section:         .eh_frame
  Global:
    - Name:            main
      Type:            STT_FUNC
      Section:         .text
      Size:            0x000000000000000C
    - Name:            foo
