- Name: .MIPS.abiflags
  Type: SHT_MIPS_ABIFLAGS
  AddressAlign: 8
  ISA:          MIPS32
  ISARevision:  2
  ISAExtension: EXT_NONE
  ASEs:         [ MICROMIPS ]
  FpABI:        FP_XX
  GPRSize:      REG_32
  CPR1Size:     REG_32
  CPR2Size:     REG_NONE
  Flags1:       [ ]
