# t1.so.o
---
FileHeader:
  Class:   ELFCLASS32
  Data:    ELFDATA2LSB
  Type:    ET_REL
  Machine: EM_MIPS
  Flags:   [EF_MIPS_PIC, EF_MIPS_CPIC, EF_MIPS_ABI_O32,
            EF_MIPS_ARCH_32R2, EF_MIPS_MICROMIPS]

Sections:
- Name:         .text
  Type:         SHT_PROGBITS
  Size:         8
  AddressAlign: 16
  Flags:        [SHF_ALLOC, SHF_EXECINSTR]

Symbols:
  Global:
    - Name:    T2
      Section: .text
      Type:    STT_FUNC
      Value:   0
      Size:    4
    - Name:    M2
      Section: .text
      Type:    STT_FUNC
      Value:   4
      Size:    4
      Other:   [STO_MIPS_MICROMIPS]

# t2.so.o
---
FileHeader:
  Class:   ELFCLASS32
  Data:    ELFDATA2LSB
  Type:    ET_REL
  Machine: EM_MIPS
  Flags:   [EF_MIPS_PIC, EF_MIPS_CPIC, EF_MIPS_ABI_O32,
            EF_MIPS_ARCH_32R2, EF_MIPS_MICROMIPS]

Sections:
- Name:         .text
  Type:         SHT_PROGBITS
  Size:         40
  AddressAlign: 16
