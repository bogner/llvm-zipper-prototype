# RUN: lld-core %s | FileCheck %s

#
# Test fixups to simple named atoms
#

---
atoms:
    - name:              foo
      type:              code
      content:           [ E8, 00, 00, 00, 00, E8, 00, 00, 00, 00 ]
      fixups:
      - offset:          1
        kind:            3
        target:          bar
      - offset:          6
        kind:            3
        target:          baz
  
    - name:              baz
      scope:             static
      type:              code

    - name:              bar
      definition:        undefined
  

...

# CHECK:      name: foo
# CHECK:      fixups:
# CHECK:      target: bar
# CHECK:      target: baz
# CHECK:      ...

