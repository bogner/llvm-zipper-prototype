# RUN: lld -core %s | FileCheck %s

#
# Test that duplicate c-strings are coalesced
#

---
defined-atoms:
    - ref-name:          L0
      type:              c-string
      merge:             by-content
      content:           [ 68, 65, 6c, 6c, 6f, 00 ]
      
    - ref-name:          L1
      type:              c-string
      merge:             by-content
      content:           [ 74, 68, 65, 72, 65, 00 ]
---
defined-atoms:
    - ref-name:          L2
      type:              c-string
      merge:             by-content
      content:           [ 68, 65, 6c, 6c, 6f, 00 ]
---
defined-atoms:
    - ref-name:          L2
      type:              c-string
      merge:             by-content
      content:           [ 74, 68, 65, 72, 65, 00 ]
...

