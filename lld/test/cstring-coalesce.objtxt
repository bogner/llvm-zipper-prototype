# RUN: lld-core %s | FileCheck %s

#
# Test that duplicate c-strings are coalesced
#

---
atoms:
    - ref-name:          L0
      scope:             hidden
      type:              c-string
      content:           [ 68, 65, 6c, 6c, 6f, 00 ]
      
    - ref-name:          L1
      scope:             hidden
      type:              c-string
      content:           [ 74, 68, 65, 72, 65, 00 ]
---
atoms:
    - ref-name:          L2
      scope:             hidden
      type:              c-string
      content:           [ 68, 65, 6c, 6c, 6f, 00 ]
---
atoms:
    - ref-name:          L2
      scope:             hidden
      type:              c-string
      content:           [ 74, 68, 65, 72, 65, 00 ]
...

