# RUN: lld-core %s | FileCheck %s

#
# Tests archives in YAML. Tests that an undefined in a regular file will load
# all atoms in select archive members.
#

---
atoms:
    - name:              foo
      type:              code
      
    - name:              bar
      definition:        undefined

---
name: bar.o
atoms:
    - name:              bar
      scope:             global
      type:              code
      
    - name:              bar2
      type:              code
  
---
name: baz.o
atoms:
    - name:              baz
      scope:             global
      type:              code
      
    - name:              baz2
      type:              code
---
kind: archive
members:  
    - name:    bar.o
    - name:    baz.o
...

# CHECK:       name:       foo
# CHECK:       name:       bar
# CHECK-NOT:   definition: undefined
# CHECK:       name:       bar2
# CHECK-NOT:   name:       baz
# CHECK:       ...
