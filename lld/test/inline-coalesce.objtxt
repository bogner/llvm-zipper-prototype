# RUN: lld -core %s | FileCheck %s

#
# Test that non-inlined inlined functions are silently coalesced
#

---
defined-atoms:
    - name:              _inlineFunc
      scope:             global
      type:              code
      merge:             as-weak
---
defined-atoms:
    - name:              _inlineFunc
      scope:             global
      type:              code
      merge:             as-weak
---
defined-atoms:
    - name:              _inlineFunc
      scope:             global
      type:              code
      merge:             as-weak
...


# CHECK:       name: _inlineFunc
# CHECK:       merge: as-weak
# CHECK-NOT:   name: _inlineFunc
# CHECK:       ...
