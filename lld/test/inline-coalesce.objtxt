# RUN: lld-core %s | FileCheck %s

#
# Test that non-inlined inlined functions are silently coalesced
#

---
atoms:
    - name:              _inlineFunc
      scope:             global
      definition:        regular
      type:              code
      merge:             asWeak
---
atoms:
    - name:              _inlineFunc
      scope:             global
      definition:        regular
      type:              code
      merge:             asWeak
---
atoms:
    - name:              _inlineFunc
      scope:             global
      definition:        regular
      type:              code
      merge:             asWeak
...


# CHECK:       name: _inlineFunc
# CHECK:       merge: asWeak
# CHECK-NOT:   name: _inlineFunc
# CHECK:       ...
