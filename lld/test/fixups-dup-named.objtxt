# RUN: lld-core %s | FileCheck %s

#
# Test fixups referencing multiple atoms that have the same name
#

---
atoms:
    - name:              foo
      type:              code
      content:           [ E8, 00, 00, 00, 00, E8, 00, 00, 00, 00 ]
      fixups:
      - offset:          1
        kind:            3
        target:          bar_1
      - offset:          6
        kind:            3
        target:          bar_2
  
    - name:              bar
      ref-name:          bar_1
      scope:             static

    - name:              bar
      ref-name:          bar_2
      scope:             static
  

...

# CHECK:       ...
