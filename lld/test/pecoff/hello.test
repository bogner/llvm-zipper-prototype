# RUN: lld -flavor link -out %t1 -subsystem console -force \
# RUN:   -- %p/Inputs/hello.obj \
# RUN:   && llvm-readobj -sections %t1 | FileCheck %s

CHECK: Format: COFF-i386
CHECK: Arch: i386
CHECK: AddressSize: 32bit
CHECK: Sections [
CHECK:   Section {
CHECK:     Number: 1
CHECK:     Name: .text (2E 74 65 78 74 00 00 00)
