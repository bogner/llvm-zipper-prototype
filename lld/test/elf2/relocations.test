# RUN: yaml2obj -format elf %s -o %t
# RUN: lld -flavor gnu2 %t -o %tout
# RUN: llvm-objdump -s %tout | FileCheck %s

FileHeader:
  Class:           ELFCLASS64
  Data:            ELFDATA2LSB
  Type:            ET_REL
  Machine:         EM_X86_64
Sections:
  - Name:            .text
    Type:            SHT_PROGBITS
    Size:            512
  - Name:            .rela.text
    Type:            SHT_RELA
    Info:            .text
    Relocations:
      - Offset:          0x0000000000000000
        Symbol:          _start
        Type:            R_X86_64_32S
      - Offset:          0x0000000000000004
        Symbol:          _start
        Type:            R_X86_64_64

Symbols:
  Global:
    - Name:            _start
      Section:         .text
      Value:           0x42

# CHECK: 0000 42000000 42000000 00000000 00000000
