# RUN: lld-core -layout-pass=true %s | FileCheck %s -check-prefix=CHKORDER

---
defined-atoms:   
  - name:            fn
    scope:           global

  - name:            fn1
    scope:           global
    references:      
      - kind:            layout-before
        offset:          0
        target:          fn
  - name:            fn2
    scope:           global
    references:      
      - kind:            layout-before
        offset:          0
        target:          fn1
...


