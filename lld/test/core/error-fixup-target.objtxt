# RUN: not lld -core %s 2> %t.err
# RUN: FileCheck %s < %t.err

#
# Test that unbindable target name produces a readable error.
#

---
defined-atoms:
    - name:          entry
      scope:         hidden
      references:
      - offset:      3
        kind:        pcrel32
        target:      bar
      - offset:      5
        kind:        pcrel32
        target:      baz

undefined-atoms:
    - name:          bar

...


# CHECK:  error: no such atom name: baz
