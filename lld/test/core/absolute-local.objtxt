# RUN: lld -core  %s  | FileCheck %s

#
# Test that absolute symbols with local scope do not cause name conflict
#
---
absolute-atoms:
    - name:              putchar
      ref-name:          pc1
      value:             0xFFFF0040
      scope:             static

    - name:              putchar
      ref-name:          pc2
      value:             0xFFFF0040
      scope:             static
...

# CHECK:     ---
# CHECK:     absolute-atoms:
# CHECK:     - name:         putchar
# CHECK:       value:        0x00000000FFFF0040
# CHECK:     - name:         putchar
# CHECK:       value:        0x00000000FFFF0040
# CHECK:     ...
