# RUN: lld -core %s | FileCheck %s

#
# Test that shared-library symbols are parsed and preserved
#

---
shared-library-atoms:
    - name:              malloc
      load-name:         libc.so
      type:              code
      size:              0

    - name:              free
      load-name:         libc.so

    - name:              fast_malloc
      load-name:         libc.so
      can-be-null:       at-runtime

    - name:              stdout
      load-name:         libc.so
      type:              data
      size:              8

...

