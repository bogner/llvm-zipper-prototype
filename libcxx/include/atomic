inline _LIBCPP_INLINE_VISIBILITY
unsigned int
atomic_fetch_sub_explicit(atomic_uint* __obj, unsigned int __v,
                          memory_order __o)
{
    return atomic_fetch_sub_explicit(const_cast<volatile atomic_uint*>(__obj),
                                                                      __v, __o);
}

