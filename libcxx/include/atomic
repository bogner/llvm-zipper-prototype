bool
atomic_compare_exchange_weak_explicit(volatile atomic_short* __obj,
                                      short* __exp, short __desr,
                                      memory_order __s, memory_order __f)
{
    return __atomic_compare_exchange_weak(&__obj->__v_, __exp, __desr, __s,
                                                                           __f);
}

